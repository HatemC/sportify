<div class="notification">
      <% if offer.user.photo.key.nil?  %>
      <%=  image_tag "default_profil.png", class:"avatar-large" %>
      <% else  %>
      <%=  cl_image_tag offer.user.photo.key, class:"avatar-large"  %>
      <% end  %>

  <div class="notification-content">
    <p><small><strong>Address: </strong><%= offer.address %></small></p>
    <p><strong>Sport: </strong><%= offer.sport %> </p>
    <p><strong>Level: </strong><%= offer.level %></p>
  </div>
</div>
